{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "watch": "rimraf ./dist && tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "dev-up": "db-migrate up -m ./_Database/migrations   --config ./_Database/config/dev.json",
    "dev-down": "db-migrate reset -m ./_Database/migrations  --config ./_Database/config/dev.json",
    "test-up": "db-migrate up -e test -m ./_Database/migrations   --config ./_Database/config/test.json",
    "test-down": "db-migrate reset  -e test -m ./_Database/migrations  --config ./_Database/config/test.json",
    "test": "npm run test-up && cross-env ENVI=test jasmine-ts && npm run test-down",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "tsc": "rimraf ./dist && tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^7.14.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@ert78gb/jasmine-ts": "^0.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.8.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "jasmine": "^3.8.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.3.5"
  }
}
